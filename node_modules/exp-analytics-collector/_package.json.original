{
  "name": "exp-analytics-collector",
  "version": "0.0.1",
  "description": "Used to send various events to the analytics-collector service",
  "repository": {
    "type": "git",
    "url": "http://geoff.berger@stash.experticity.com/scm/plat/exp-analytics-collector.git"
  },
  "main": "dist/analytics-collector.js",
  "scripts": {
    "test": "npm run eslint && npm run karma",
    "build": "rm -rf dist && npm run webpack",
    "webpack": "npm run webpack:unminify && npm run webpack:minify",
    "webpack:unminify": "webpack",
    "webpack:die-ie8-die": "webpack --config webpack.config.die-ie8-die.js",
    "webpack:minify": "webpack --config webpack.config.min.js",
    "karma": "karma start --reporters junit,mocha --single-run --browsers PhantomJS --no-colors",
    "karma:watch": "karma start",
    "eslint": "npm run eslint:src && npm run eslint:test",
    "eslint:src": "eslint src",
    "eslint:test": "eslint --env mocha test",
    "start": "nodemon --exec babel-node -- examples/app.js"
  },
  "author": "TeamSkyNet@experticity.com",
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.4.1",
    "chai-subset": "^1.1.0",
    "deep-extend": "^0.4.0",
    "es5-shim": "^4.3.1",
    "eslint": "^1.9.0",
    "exp-publish-node": "^0.1.2",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-junit-reporter": "^0.3.8",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "koa": "^1.1.2",
    "koa-body": "^1.3.0",
    "koa-hbs": "^0.7.0",
    "koa-router": "^5.2.3",
    "koa-static": "^1.5.2",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "phantomjs": "^1.9.18",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.6"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
